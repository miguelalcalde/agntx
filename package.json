{
  "name": "agntx",
  "version": "1.0.5",
  "description": "Your agent files manager. Install skills, agents, commands, and more from GitHub repositories or local paths.",
  "main": "dist/index.js",
  "bin": {
    "agntx": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "changeset": "changeset",
    "changeset:status": "changeset status --verbose",
    "version:packages": "changeset version",
    "pack:check": "npm pack --dry-run",
    "secrets:scan": "gitleaks git --verbose --redact",
    "release:check": "pnpm run build && pnpm run test && pnpm run pack:check",
    "prepublishOnly": "pnpm run release:check",
    "test": "node dist/index.js --version && node dist/index.js --help > /dev/null",
    "prepare": "husky"
  },
  "keywords": [
    "agent",
    "cli",
    "cursor",
    "claude",
    "codex"
  ],
  "author": "mikemajara",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miguelalcalde/agntx.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^14.0.3",
    "glob": "^11.0.2",
    "gray-matter": "^4.0.3",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "simple-git": "^3.30.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@types/inquirer": "^8.2.10",
    "@types/node": "^22.15.0",
    "husky": "^9.1.7",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.6.2"
}
